<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.example.moviles.proyectomoviles.Fragments.Register">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="0dp">

        <TextView
            android:id="@+id/tvNombre"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/nombres"
            app:layout_constraintRight_toLeftOf="@+id/nameLogup"
            tools:layout_constraintRight_creator="1"
            android:layout_marginEnd="26dp"
            app:layout_constraintBaseline_toBaselineOf="@+id/nameLogup"
            tools:layout_constraintBaseline_creator="1"
            android:layout_marginRight="26dp" />

        <TextView
            android:id="@+id/tvApellidos"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/apellidos"
            app:layout_constraintRight_toLeftOf="@+id/lastnameLogup"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toBottomOf="@+id/lastnameLogup"
            android:layout_marginEnd="26dp"
            android:layout_marginBottom="11dp"
            android:layout_marginRight="26dp" />

        <TextView
            android:id="@+id/tvCorreo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/correo"
            app:layout_constraintRight_toLeftOf="@+id/correo"
            tools:layout_constraintRight_creator="1"
            android:layout_marginEnd="26dp"
            app:layout_constraintBaseline_toBaselineOf="@+id/correo"
            tools:layout_constraintBaseline_creator="1"
            android:layout_marginRight="26dp" />

        <TextView
            android:id="@+id/tvPass"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Sexo"
            app:layout_constraintRight_toLeftOf="@+id/sexo"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toBottomOf="@+id/sexo"
            android:layout_marginEnd="26dp"
            android:layout_marginBottom="11dp"
            android:layout_marginRight="26dp" />

        <TextView
            android:id="@+id/tvRePass"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="DNI"
            app:layout_constraintRight_toLeftOf="@+id/dni"
            tools:layout_constraintRight_creator="1"
            android:layout_marginEnd="26dp"
            app:layout_constraintBaseline_toBaselineOf="@+id/dni"
            tools:layout_constraintBaseline_creator="1"
            android:layout_marginRight="26dp" />

        <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline"
            android:orientation="vertical"
            app:layout_constraintGuide_begin="127dp"
            tools:layout_editor_absoluteY="25dp"
            tools:layout_editor_absoluteX="127dp" />

        <Button
            android:id="@+id/logup"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/registro"
            app:layout_constraintRight_toLeftOf="@+id/telefono"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            android:layout_marginTop="14dp"
            app:layout_constraintTop_toBottomOf="@+id/telefono" />

        <EditText
            android:id="@+id/correo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textPassword"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            app:layout_constraintRight_toRightOf="@+id/dni"
            android:layout_marginTop="19dp"
            app:layout_constraintTop_toBottomOf="@+id/sexo"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="@+id/dni" />

        <EditText
            android:id="@+id/dni"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textPassword"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            android:layout_marginEnd="16dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginTop="18dp"
            app:layout_constraintTop_toBottomOf="@+id/correo"
            android:layout_marginRight="16dp" />

        <EditText
            android:id="@+id/sexo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textEmailAddress"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            app:layout_constraintRight_toRightOf="@+id/correo"
            android:layout_marginTop="14dp"
            app:layout_constraintTop_toBottomOf="@+id/lastnameLogup"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="@+id/correo" />

        <EditText
            android:id="@+id/lastnameLogup"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:hint="Escriba sus apellidos"
            android:inputType="textPersonName"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            app:layout_constraintRight_toRightOf="@+id/sexo"
            android:layout_marginTop="15dp"
            app:layout_constraintTop_toBottomOf="@+id/nameLogup"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="@+id/sexo" />

        <EditText
            android:id="@+id/nameLogup"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:hint="Escriba sus nombres"
            android:inputType="textPersonName"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            app:layout_constraintRight_toRightOf="@+id/lastnameLogup"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/register_error"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="@+id/lastnameLogup" />

        <TextView android:id="@+id/register_error"
            android:layout_width="0dp"
            android:layout_height="wrap_content"

            android:gravity="center"
            android:textSize="20dip"
            android:textColor="#0b84aa"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginTop="25dp"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline2"
            android:orientation="vertical"
            app:layout_constraintGuide_end="233dp"
            tools:layout_editor_absoluteY="25dp"
            tools:layout_editor_absoluteX="151dp" />

        <Button
            android:id="@+id/login"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Login"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            android:layout_marginStart="74dp"
            android:layout_marginEnd="69dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginTop="14dp"
            app:layout_constraintTop_toBottomOf="@+id/telefono"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="@+id/telefono"
            android:layout_marginLeft="74dp"
            android:layout_marginRight="69dp" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Oficina"
            app:layout_constraintRight_toLeftOf="@+id/oficina"
            tools:layout_constraintRight_creator="1"
            android:layout_marginEnd="28dp"
            app:layout_constraintBaseline_toBaselineOf="@+id/oficina"
            tools:layout_constraintBaseline_creator="1"
            android:layout_marginRight="28dp" />

        <EditText
            android:id="@+id/telefono"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            app:layout_constraintRight_toRightOf="@+id/dni"
            android:layout_marginTop="81dp"
            app:layout_constraintTop_toBottomOf="@+id/dni"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="@+id/dni" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Telefono"
            android:layout_marginStart="5dp"
            app:layout_constraintBaseline_toBaselineOf="@+id/telefono"
            tools:layout_constraintBaseline_creator="1"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="@+id/logup"
            android:layout_marginLeft="5dp" />

        <EditText
            android:id="@+id/oficina"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toTopOf="@+id/telefono"
            android:layout_marginEnd="14dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginBottom="17dp"
            android:layout_marginRight="14dp" />

    </android.support.constraint.ConstraintLayout>
</FrameLayout>