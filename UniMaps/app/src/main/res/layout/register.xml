<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.proyectomoviles.unimaps.Fragments.Register">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="0dp">

        <TextView
            android:id="@+id/tNombres"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/nombres"
            tools:layout_constraintRight_creator="1"
            app:layout_constraintRight_toRightOf="@+id/tApellidos"
            app:layout_constraintBaseline_toBaselineOf="@+id/eNombres"
            tools:layout_constraintBaseline_creator="1"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="@+id/tApellidos" />

        <TextView
            android:id="@+id/tApellidos"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/apellidos"
            android:layout_marginEnd="26dp"
            app:layout_constraintRight_toLeftOf="@+id/eApellidos"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            android:layout_marginTop="33dp"
            app:layout_constraintTop_toBottomOf="@+id/tNombres" />

        <TextView
            android:id="@+id/tCorreo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/correo"
            android:layout_marginEnd="26dp"
            app:layout_constraintRight_toLeftOf="@+id/eCorreo"
            tools:layout_constraintRight_creator="1"
            app:layout_constraintBaseline_toBaselineOf="@+id/eCorreo"
            tools:layout_constraintBaseline_creator="1" />

        <TextView
            android:id="@+id/tSexo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Sexo"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toBottomOf="@+id/spinnerSexo"
            app:layout_constraintRight_toRightOf="@+id/tEmpresa" />

        <TextView
            android:id="@+id/tEmpresa"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Empresa"
            android:layout_marginEnd="26dp"
            app:layout_constraintRight_toLeftOf="@+id/eEmpresa"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/eEmpresa" />

        <TextView
            android:id="@+id/tOficina"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Oficina"
            android:layout_marginEnd="5dp"
            tools:layout_constraintRight_creator="1"
            app:layout_constraintRight_toRightOf="@+id/tContrasenia"
            app:layout_constraintBaseline_toBaselineOf="@+id/eOficina"
            tools:layout_constraintBaseline_creator="1" />

        <TextView
            android:id="@+id/tContrasenia"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Contrasenia"
            android:layout_marginEnd="26dp"
            app:layout_constraintRight_toLeftOf="@+id/ePassword"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            android:layout_marginTop="9dp"
            app:layout_constraintTop_toTopOf="@+id/ePassword" />

        <TextView
            android:id="@+id/tSede"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Sede"
            android:layout_marginEnd="26dp"
            app:layout_constraintRight_toLeftOf="@+id/eSede"
            tools:layout_constraintRight_creator="1"
            app:layout_constraintBaseline_toBaselineOf="@+id/eSede"
            tools:layout_constraintBaseline_creator="1" />

        <TextView
            android:id="@+id/tDependencia"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Dependencia"
            android:layout_marginEnd="26dp"
            app:layout_constraintRight_toLeftOf="@+id/eDependencia"
            tools:layout_constraintRight_creator="1"
            app:layout_constraintBaseline_toBaselineOf="@+id/eDependencia"
            tools:layout_constraintBaseline_creator="1" />

        <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline"
            android:orientation="vertical"
            app:layout_constraintGuide_begin="127dp"
            tools:layout_editor_absoluteY="25dp"
            tools:layout_editor_absoluteX="127dp" />

        <EditText
            android:id="@+id/eNombres"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:hint="Escriba sus nombres"
            android:inputType="textPersonName"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            app:layout_constraintRight_toRightOf="@+id/eApellidos"
            android:layout_marginTop="51dp"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="@+id/eApellidos"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/eApellidos"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:hint="Escriba sus apellidos"
            android:inputType="textPersonName"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            app:layout_constraintRight_toRightOf="@+id/eCorreo"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/eNombres"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="@+id/eCorreo" />

        <EditText
            android:id="@+id/eCorreo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textEmailAddress"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            app:layout_constraintRight_toRightOf="@+id/eEmpresa"
            android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@+id/eApellidos"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="@+id/eEmpresa" />

        <EditText
            android:id="@+id/eEmpresa"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textMultiLine"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            app:layout_constraintRight_toRightOf="@+id/eSede"
            android:layout_marginTop="55dp"
            app:layout_constraintTop_toBottomOf="@+id/eCorreo"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="@+id/eSede" />

        <EditText
            android:id="@+id/eSede"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textMultiLine"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            app:layout_constraintRight_toRightOf="@+id/eDependencia"
            android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@+id/eEmpresa"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="@+id/eDependencia" />

        <EditText
            android:id="@+id/eDependencia"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textMultiLine"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            app:layout_constraintRight_toRightOf="@+id/eOficina"
            android:layout_marginTop="12dp"
            app:layout_constraintTop_toBottomOf="@+id/eSede"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="@+id/eOficina" />

        <EditText
            android:id="@+id/ePassword"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textPassword"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            android:layout_marginEnd="16dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginTop="13dp"
            app:layout_constraintTop_toBottomOf="@+id/eOficina" />

        <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline2"
            android:orientation="vertical"
            app:layout_constraintGuide_end="233dp"
            tools:layout_editor_absoluteY="25dp"
            tools:layout_editor_absoluteX="151dp" />

        <Spinner
            android:id="@+id/spinnerSexo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:layout_constraintTop_creator="1"
            android:layout_marginTop="12dp"
            app:layout_constraintTop_toBottomOf="@+id/eCorreo"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="@+id/eCorreo" />

        <EditText
            android:id="@+id/eOficina"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            app:layout_constraintRight_toRightOf="@+id/ePassword"
            android:layout_marginTop="13dp"
            app:layout_constraintTop_toBottomOf="@+id/eDependencia"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="@+id/ePassword" />

        <Button
            android:id="@+id/bRegister"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/registro"
            android:layout_marginStart="21dp"
            tools:layout_constraintTop_creator="1"
            android:layout_marginTop="11dp"
            app:layout_constraintTop_toBottomOf="@+id/ePassword"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toRightOf="@+id/tContrasenia" />

        <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline5"
            app:layout_constraintGuide_begin="20dp"
            android:orientation="horizontal"
            tools:layout_editor_absoluteY="45dp"
            tools:layout_editor_absoluteX="0dp" />

    </android.support.constraint.ConstraintLayout>
</FrameLayout>