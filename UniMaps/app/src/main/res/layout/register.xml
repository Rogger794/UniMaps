<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.proyectomoviles.unimaps.Fragments.Register">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="0dp">

        <TextView
            android:id="@+id/tNombres"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/nombres"
            android:layout_marginEnd="25dp"
            app:layout_constraintRight_toLeftOf="@+id/eNombres"
            tools:layout_constraintRight_creator="1"
            app:layout_constraintBaseline_toBaselineOf="@+id/eNombres"
            tools:layout_constraintBaseline_creator="1"
            android:layout_marginRight="26dp" />

        <TextView
            android:id="@+id/tApellidos"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/apellidos"
            android:layout_marginEnd="26dp"
            app:layout_constraintRight_toLeftOf="@+id/eApellidos"
            tools:layout_constraintRight_creator="1"
            app:layout_constraintBaseline_toBaselineOf="@+id/eApellidos"
            tools:layout_constraintBaseline_creator="1"
            android:layout_marginRight="31dp" />

        <TextView
            android:id="@+id/tCorreo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/correo"
            android:layout_marginEnd="25dp"
            app:layout_constraintRight_toLeftOf="@+id/eCorreo"
            tools:layout_constraintRight_creator="1"
            app:layout_constraintBaseline_toBaselineOf="@+id/eCorreo"
            tools:layout_constraintBaseline_creator="1"
            android:layout_marginRight="24dp" />

        <TextView
            android:id="@+id/tSexo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Sexo"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toBottomOf="@+id/spinnerSexo"
            app:layout_constraintRight_toRightOf="@+id/tEmpresa"
            android:layout_marginRight="-1dp"
            android:layout_marginBottom="10dp" />

        <TextView
            android:id="@+id/tEmpresa"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Empresa"
            app:layout_constraintRight_toLeftOf="@+id/eEmpresa"
            tools:layout_constraintRight_creator="1"
            android:layout_marginEnd="25dp"
            app:layout_constraintBaseline_toBaselineOf="@+id/eEmpresa"
            tools:layout_constraintBaseline_creator="1"
            android:layout_marginRight="27dp" />

        <TextView
            android:id="@+id/tOficina"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Oficina"
            android:layout_marginEnd="32dp"
            app:layout_constraintRight_toLeftOf="@+id/eOficina"
            tools:layout_constraintRight_creator="1"
            app:layout_constraintBaseline_toBaselineOf="@+id/eOficina"
            tools:layout_constraintBaseline_creator="1"
            android:layout_marginRight="38dp" />

        <TextView
            android:id="@+id/tContrasenia"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Contrasenia"
            android:layout_marginEnd="26dp"
            app:layout_constraintRight_toLeftOf="@+id/ePassword"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toBottomOf="@+id/ePassword"
            android:layout_marginBottom="18dp"
            android:layout_marginRight="26dp" />

        <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline"
            android:orientation="vertical"
            app:layout_constraintGuide_begin="127dp"
            tools:layout_editor_absoluteY="25dp"
            tools:layout_editor_absoluteX="127dp" />

        <EditText
            android:id="@+id/eNombres"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:hint="Escriba sus nombres"
            android:inputType="textPersonName"
            android:layout_marginEnd="16dp"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toTopOf="@+id/eApellidos"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginBottom="10dp"
            android:layout_marginRight="16dp" />

        <EditText
            android:id="@+id/eApellidos"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:hint="Escriba sus apellidos"
            android:inputType="textPersonName"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toTopOf="@+id/eCorreo"
            app:layout_constraintRight_toRightOf="@+id/eNombres"
            tools:layout_constraintLeft_creator="1"
            android:layout_marginBottom="9dp"
            app:layout_constraintLeft_toLeftOf="@+id/eNombres"
            app:layout_constraintHorizontal_bias="0.0" />

        <EditText
            android:id="@+id/eCorreo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textEmailAddress"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toTopOf="@+id/eEmpresa"
            app:layout_constraintRight_toRightOf="@+id/eApellidos"
            tools:layout_constraintLeft_creator="1"
            android:layout_marginBottom="59dp"
            app:layout_constraintLeft_toLeftOf="@+id/eApellidos"
            app:layout_constraintHorizontal_bias="0.0" />

        <EditText
            android:id="@+id/eEmpresa"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textMultiLine"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toTopOf="@+id/eOficina"
            app:layout_constraintRight_toRightOf="@+id/eCorreo"
            tools:layout_constraintLeft_creator="1"
            android:layout_marginBottom="17dp"
            app:layout_constraintLeft_toLeftOf="@+id/eCorreo"
            app:layout_constraintHorizontal_bias="0.0" />

        <EditText
            android:id="@+id/ePassword"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textPassword"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="@+id/eOficina"
            tools:layout_constraintLeft_creator="1"
            android:layout_marginBottom="125dp"
            app:layout_constraintLeft_toLeftOf="@+id/eOficina"
            app:layout_constraintHorizontal_bias="0.0" />

        <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline2"
            android:orientation="vertical"
            app:layout_constraintGuide_end="233dp"
            tools:layout_editor_absoluteY="25dp"
            tools:layout_editor_absoluteX="151dp" />

        <Spinner
            android:id="@+id/spinnerSexo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toTopOf="@+id/eEmpresa"
            tools:layout_constraintLeft_creator="1"
            android:layout_marginBottom="10dp"
            app:layout_constraintLeft_toLeftOf="@+id/eEmpresa"
            android:layout_marginLeft="12dp" />

        <EditText
            android:id="@+id/eOficina"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toTopOf="@+id/ePassword"
            app:layout_constraintRight_toRightOf="@+id/eEmpresa"
            tools:layout_constraintLeft_creator="1"
            android:layout_marginBottom="14dp"
            app:layout_constraintLeft_toLeftOf="@+id/eEmpresa"
            app:layout_constraintHorizontal_bias="0.0" />

        <Button
            android:id="@+id/bRegister"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/registro"
            android:layout_marginStart="21dp"
            tools:layout_constraintTop_creator="1"
            android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/ePassword"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toRightOf="@+id/tContrasenia"
            android:layout_marginLeft="21dp" />

        <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline5"
            app:layout_constraintGuide_begin="20dp"
            android:orientation="horizontal"
            tools:layout_editor_absoluteY="45dp"
            tools:layout_editor_absoluteX="0dp" />

    </android.support.constraint.ConstraintLayout>
</FrameLayout>